{% set custom = get_customisation() %}
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('/images/favicon-96x96.png') }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <META NAME="TITLE" CONTENT="{{ custom.title }}">
    <META NAME="AUTHOR" CONTENT="Baptiste BUVRON">
    <META NAME="DESCRIPTION" CONTENT="{{ custom.description }}">
    <META NAME="KEYWORDS" CONTENT="SGDF, scout, compagnon, solidaritÃ©, action, humanitaire, scoutisme, scoutes et guides de france, scouting">
